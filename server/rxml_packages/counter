<define tag="xcounter">
<attrib name="font">counter_a</attrib>
<gtext font="&_.font;" ::="&_.rest-args;"><accessed ::="&_.rest-args;"/></gtext>
</define>

<define tag="xcounter-url">
<attrib name="font">counter_a</attrib>
<gtext-url font="&_.font;" ::="&_.rest-args;"><accessed ::="&_.rest-args;"/></gtext-url>
</define>