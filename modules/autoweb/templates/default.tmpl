<tmplblock tag=error>
  <table border=0><tr>
    <td valign=top><img src="/internal-roxen-err_3"></td>
    <td><font color=darkred>$error$</font></td>
  </tr></table>
</tmplblock>

<tmplblock tag=browser>
  <comment><font size=+1><b>File Browser</b></font><br></comment>
  $browser$
</tmplblock>

<tmplblock tag=filelistning>
  $filelistning$
</tmplblock>

<tmplblock tag=toolbargtext params=magic,href,target>
  <gtext xspacing=12 yspacing=3 notrans nfont=helvetica scale=0.35 bold
       xsize=400 bevel=5 talign=center
       fg=#ffffff bg=#113377 magic_fg=#000000 magic_bg=#88ccaa
       magic='$magic$' href='$href$' target=$target$>$toolbargtext$</gtext></tmplblock>

<tmplblock tag=webadmtablify>
  <tablify nice cellseparator="|||" rowseparator="///">
    $webadmtablify$
  </tablify>
</tmplblock>
	
