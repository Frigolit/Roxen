<include file=gtext_headers.inc>
<include file=shadow.inc>
<include file=bg_image.inc>

<formoutput quote=£>

<html>
<head><title><as-meta var=title></title></head>
<body bgcolor=$$bg_color$$ text=$$text_color$$ link=$$link_color$$
      vlink=$$vlink_color$$ alink=$$alink_color$$ £bg_image£>
  <table cellpadding=0 cellspacing=0 border=0><!--  4 columns  -->
    <tr><!--  title  -->
      <td colspan=4 align=$$title_align$$>
        <gtext bg=$$bg_color$$ fg=$$title_color$$
	       scale=$$title_size$$
	       £title_shadow£ $$title_font_style$$
               nfont=$$title_font$$><as-meta var=title></gtext></td>
    </tr>
    <tr><!--  bar  -->
      <td colspan=4 bgcolor=$$page_head_bar_color$$><img
        src="/internal-roxen-unit" width=100% height=8></td>
    </tr>
    <tr><!--  image  -->
      <td></td>
      <td></td>
      <td colspan=2>$$page_head_image$$</td>
    </tr>
    <tr>
      <td height=10></td>
    </tr>
    <tr><!--  Navigation/Content -->
      <td valign=top><img src="/internal-roxen-unit" height=80 width=1>
        <include file='$$nav_name$$'></td>
      <td width=20%></td>
      <td><tmplinsert tag=content></td>
      <td width=20%></td>
    </tr>
  </table>
</body>
</html>

</formoutput>